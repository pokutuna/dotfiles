[github]
  user = pokutuna
[color]
  ui = auto
[alias]
  co = checkout
  st = status
  diffw = diff --word-diff
  stat = log -- stat -- summary
  hist = log --pretty=format:\"%h %ad | %s%Cgreen%d%Creset [%an]\" --decorate --graph --date=short
  #hist = log --date=short --graph --pretty=oneline --decorate --abbrev-commit --branches
  type = cat-file -t
  dump = cat-file -p
  oneline = log --oneline --decorate
  has-merged   = "!sh -c 'git rev-list HEAD | grep `git rev-parse $0`'"
  clone-branch = "!sh -c 'git checkout -t origin/$0'"
  current-branch = "!sh -c \"git branch | grep '*' | awk '{ print \\$2 }' \""%  
[diff "nkf"]
  textconv = nkf -w
